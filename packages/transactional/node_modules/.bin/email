#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/mac/Desktop/tramonafinal/tramona/node_modules/.pnpm/react-email@2.1.6_@swc+helpers@0.5.5_eslint@8.57.1_ts-node@10.9.2_@swc+core@1.3.101_@swc+help_cdryvybjdopkjd3cjwd7kdp7lu/node_modules/react-email/cli/node_modules:/Users/mac/Desktop/tramonafinal/tramona/node_modules/.pnpm/react-email@2.1.6_@swc+helpers@0.5.5_eslint@8.57.1_ts-node@10.9.2_@swc+core@1.3.101_@swc+help_cdryvybjdopkjd3cjwd7kdp7lu/node_modules/react-email/node_modules:/Users/mac/Desktop/tramonafinal/tramona/node_modules/.pnpm/react-email@2.1.6_@swc+helpers@0.5.5_eslint@8.57.1_ts-node@10.9.2_@swc+core@1.3.101_@swc+help_cdryvybjdopkjd3cjwd7kdp7lu/node_modules:/Users/mac/Desktop/tramonafinal/tramona/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/mac/Desktop/tramonafinal/tramona/node_modules/.pnpm/react-email@2.1.6_@swc+helpers@0.5.5_eslint@8.57.1_ts-node@10.9.2_@swc+core@1.3.101_@swc+help_cdryvybjdopkjd3cjwd7kdp7lu/node_modules/react-email/cli/node_modules:/Users/mac/Desktop/tramonafinal/tramona/node_modules/.pnpm/react-email@2.1.6_@swc+helpers@0.5.5_eslint@8.57.1_ts-node@10.9.2_@swc+core@1.3.101_@swc+help_cdryvybjdopkjd3cjwd7kdp7lu/node_modules/react-email/node_modules:/Users/mac/Desktop/tramonafinal/tramona/node_modules/.pnpm/react-email@2.1.6_@swc+helpers@0.5.5_eslint@8.57.1_ts-node@10.9.2_@swc+core@1.3.101_@swc+help_cdryvybjdopkjd3cjwd7kdp7lu/node_modules:/Users/mac/Desktop/tramonafinal/tramona/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../react-email/cli/index.js" "$@"
else
  exec node  "$basedir/../react-email/cli/index.js" "$@"
fi
