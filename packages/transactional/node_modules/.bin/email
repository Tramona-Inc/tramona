#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/adamkim/tramona/node_modules/.pnpm/react-email@2.1.6_eslint@8.57.0_ts-node@10.9.2/node_modules/react-email/cli/node_modules:/Users/adamkim/tramona/node_modules/.pnpm/react-email@2.1.6_eslint@8.57.0_ts-node@10.9.2/node_modules/react-email/node_modules:/Users/adamkim/tramona/node_modules/.pnpm/react-email@2.1.6_eslint@8.57.0_ts-node@10.9.2/node_modules:/Users/adamkim/tramona/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/adamkim/tramona/node_modules/.pnpm/react-email@2.1.6_eslint@8.57.0_ts-node@10.9.2/node_modules/react-email/cli/node_modules:/Users/adamkim/tramona/node_modules/.pnpm/react-email@2.1.6_eslint@8.57.0_ts-node@10.9.2/node_modules/react-email/node_modules:/Users/adamkim/tramona/node_modules/.pnpm/react-email@2.1.6_eslint@8.57.0_ts-node@10.9.2/node_modules:/Users/adamkim/tramona/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../react-email/cli/index.js" "$@"
else
  exec node  "$basedir/../react-email/cli/index.js" "$@"
fi
